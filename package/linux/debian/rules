#!/usr/bin/make -f

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

.PHONY:

%:
	dh $@

override_dh_auto_configure:
	autoconf
	dh_auto_configure -- --enable-docs --disable-graphicsmagick-config --with-tesseract --with-cuneiform --with-cuneiform-lib=/usr/lib/cuneiform --datadir='$${datarootdir}/$${PACKAGE_NAME}' --docdir='$${datarootdir}/doc/$${PACKAGE_NAME}' 

override_dh_shlibdeps:
	dh_shlibdeps -- --ignore-missing-info
